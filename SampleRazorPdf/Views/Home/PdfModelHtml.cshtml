@model  List<SampleRazorPdf.Controllers.HomeController.Person>
@{
    Layout = "~/Views/Shared/_PdfLayout.cshtml";
}

    <h1>@ViewBag.Title</h1>
    <table style="width:100% ;font-size:12px;">
            <tr>
                <th>Name</th>
                <th>City</th>
                <th>Title</th>
            </tr>

            @foreach (var p in Model)
            {
                <tr>
                    <td>@p.Name</td>
                    <td>@p.City</td>
                    <td></td>
                </tr>
            }
    </table>
    
